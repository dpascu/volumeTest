{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "implicit-arrow-linebreak": 0,
    "function-paren-newline": 0,
    "no-restricted-syntax": 0,
    "no-await-in-loop": 0,
    "object-curly-newline": 0,
    "no-mixed-operators": 0,
    "no-console": 0,
    "no-underscore-dangle": 0,
    "no-global-assign": 0,
    "import/prefer-default-export": 0,
    "import/no-named-as-default": 0,
    "max-lines": [
      "error",
      {
        "max": 250,
        "skipBlankLines": true,
        "skipComments": true
      }
    ],
    "max-len": [
      "error",
      110,
      {
        "ignoreComments": true
      }
    ],
    "curly": 0,
    "consistent-return": 0,
    "arrow-parens": 0,
    "no-return-assign": 0,
    "comma-dangle": 0,
    "no-multi-str": 0
  }
}